<com.getbase.floatingactionbutton.FloatingActionsMenu xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/multiple_actions"
    android:layout_width="200dp"
    android:layout_height="200dp"
    android:layout_alignParentBottom="true"
    android:layout_alignParentEnd="true"
    android:layout_alignParentRight="true"
    android:layout_gravity="bottom|right"
    android:layout_marginBottom="@dimen/float_button_margin_bottom"
    android:layout_marginRight="@dimen/float_button_margin_right"
    app:fab_addButtonSize="normal"
    fab:fab_addButtonColorNormal="@color/accent"
    fab:fab_addButtonColorPressed="@color/accent1"

    fab:fab_labelStyle="@style/menu_labels_style">


    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_removead"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/red"
        fab:fab_colorPressed="@color/accent1"
        fab:fab_icon="@drawable/cancel51"
        fab:fab_size="mini"

        fab:fab_title="@string/remove_ads" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_feedback"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/accent"
        fab:fab_colorPressed="@color/accent1"
        fab:fab_icon="@drawable/send4"

        fab:fab_size="mini"
        fab:fab_title="@string/feed_back" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_favorite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/accent"
        fab:fab_colorPressed="@color/accent1"
        fab:fab_icon="@drawable/mark1"
        fab:fab_size="mini"

        fab:fab_title="@string/favorite" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/fab_deactive"
        fab:fab_colorPressed="@color/fab_deactive_press"
        fab:fab_icon="@drawable/create3"
        fab:fab_size="mini"

        fab:fab_title="@string/test" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_myconcept"
        android:layout_width="wrap_content"

        android:layout_height="wrap_content"
        fab:fab_colorNormal="@color/fab_deactive"
        fab:fab_colorPressed="@color/fab_deactive_press"
        fab:fab_icon="@drawable/medical50"
        fab:fab_size="mini"
        fab:fab_title="@string/my_topic" />

</com.getbase.floatingactionbutton.FloatingActionsMenu>
